# Groq API Configuration
# Get your free API key at: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Groq API settings
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_API_URL=https://api.groq.com/openai/v1/chat/completions

# Alternative models (check Groq docs for availability):
# GROQ_MODEL=llama-3.2-90b-text-preview
# GROQ_MODEL=mixtral-8x7b-32768
# GROQ_MODEL=gemma2-9b-it

# Temperature and max tokens (lower = less tokens, faster)
GROQ_TEMPERATURE=0.4
GROQ_MAX_TOKENS=2000

# Rate limit management
GROQ_RETRY_LIMIT=3
GROQ_BACKOFF_SECONDS=2
GROQ_BETWEEN_TASKS_SECONDS=2
GROQ_BETWEEN_ITER_SECONDS=2
GROQ_BETWEEN_ACTIONS_SECONDS=1

# Debug mode (set to true to enable detailed logging in .gsdgc/debug.log)
DEBUG_MODE=false
